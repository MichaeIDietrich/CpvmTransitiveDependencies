<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
  
    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>

    <Platforms>x64</Platforms>
    <Platform>x64</Platform>
    <RuntimeIdentifiers>win-x64</RuntimeIdentifiers>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>

    <OutputPath>$(MSBuildThisFileDirectory)\..\bin\$(MSBuildProjectName)\</OutputPath>
    <IntermediateOutputPath>$(MSBuildThisFileDirectory)\..\obj\$(MSBuildProjectName)\</IntermediateOutputPath>
    <MSBuildProjectExtensionsPath>$(MSBuildThisFileDirectory)\..\obj\$(MSBuildProjectName)\</MSBuildProjectExtensionsPath>
    <PublishDir>$(MSBuildThisFileDirectory)\..\publish\$(MSBuildProjectName)\</PublishDir>

    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>

  </PropertyGroup>
</Project>